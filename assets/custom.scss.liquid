        font-family: var(--FONT-STACK-NAV);
        font-style: var(--FONT-STYLE-NAV);
        font-weight: var(--FONT-WEIGHT-NAV-BOLD);
        font-size: calc(var(--font-2) * var(--FONT-ADJUST-NAV));
    }
    .product-item-bottom{
        display: flex;
        justify-content: space-between;
        align-items: center;
        margin-top: 16px;
        font-size: calc(var(--font-2) * var(--FONT-ADJUST-NAV));
        .product-item-left{
            .product-sku, .product-qty{
                font-size: calc(var(--font-2) * var(--FONT-ADJUST-NAV));
