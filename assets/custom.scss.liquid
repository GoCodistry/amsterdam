    .header__mobile,
    .page-announcement,
    .header__desktop__lower,
    #preview-bar-iframe,
    .related__wrapper,
    #cart-bar,
    .footer__block:not(:first-child),
    .footer__bottom,
    .product-accordion .accordion-row:not(:first-child),
    .variant__countdown,
