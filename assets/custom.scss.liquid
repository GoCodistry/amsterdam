    .product__sharing, .product-form{
        display: none !important;
    }
    .header__desktop{
        display: block !important;
        opacity: 1 !important;
        visibility: visible !important;
        max-height: unset !important;    
    }
    .header__desktop__upper{
        & > div:nth-child(1),
        & > div:nth-child(3){
