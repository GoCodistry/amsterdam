}
.select-popout__item.hidden{
    display: none;
}
.product__title{
    font-size: 18px;
}
.product-additional-button.btn{
    width: calc(50% - 5px);
    border: 1px solid var(--primary);
    &.hold-item{
        color: #707070;
    }
    &:hover{
        border-color: #dbdbdb;
    }
}
.product__sharing{
    display: flex;
    justify-content: space-between;
    flex-wrap: nowrap;
    border-top: 1px solid #707070;
    margin-top: 25px;
    padding-top: 25px;
}
.product__page .product__sharing .share{
    opacity: 1;
    visibility: visible;
}
.product__page .product__sharing .share svg{
    width: 20px;
    height: 20px;
}
.product__page .product__sharing .share svg path:first-child{
    fill: #252525;
    stroke: none;
}
.product__page .product__sharing .share svg path + path{
    stroke: white;
}
.product__page .product__sharing .share.share--link svg path:first-child,
.product__page .product__sharing .share.share--link svg path + path{
    fill: none;
    stroke: #252525;
}
.product__sharing .product-sharing__social{
    display: flex;
    flex-wrap: wrap;
    align-items: center;
    gap: 10px;
}
.product__sharing .product-sharing__print svg{
    width: 20px;
    height: 20px;
}
.product__sharing{
    .share--link{
        position: relative;
        .copied{
            display: none;
            position: absolute;
            right: 0;
            top: 100%;
            background: white;
            border: 1px solid #707070;
            padding: 7.5px 10px;
            font-size: 14px;
            margin-top: 10px;
            color: #424242;
        }
    }
    .product-sharing__print{
        position: relative;
        .print-options{
            position: absolute;
            top:100%;
            left:0;
            border: 1px solid #707070;
            padding: 5px 0;
            display: none;
            &:before{
                content:'';
                display: block;
                width: 0; 
                height: 0; 
                border-left: 7px solid transparent;
                border-right: 7px solid transparent;
                border-bottom: 7px solid #707070;
                position: absolute;
                top: -7px;
                left: 2px;
            }
            &:after{
                content:'';
                display: block;
                width: 0; 
                height: 0; 
                border-left: 7px solid transparent;
                border-right: 7px solid transparent;
                border-bottom: 7px solid white;
                position: absolute;
                top: -5.5px;
                left: 2px;
            }
            a{
                color: #424242;
                white-space: nowrap;
                display: block;
                padding:5px 10px;
                &:hover{
                    background: #eaeaea;
                }
            }
        }
        &.expanded{
            .print-options{
                display: block;
            }
        }
    }
    
}

#popup1,#popup2{
    display:none;
}
.overlay{
    .overlay-blocker{
        content: "";
        display: block;
        position: fixed;
        top: 0;
        left: 0;
        width: 100%;
        height: 100%;
        background-color: rgba(255,255,255,0.8);
        z-index: 99999999;
    }
}
.popup{
    padding: 20px;
    background: #fff;
    border-radius: 5px;
    top: 50%;
    left: 50%;
    width: 90%;
    max-width: 520px;
    position: fixed;
    z-index: 999999999;
    border: 1px solid #ccc;
    margin: 0 auto;
    overflow-y: scroll;
    max-height: 650px;
    height: 90%;
    position: fixed;
    top: 50%;
    left: 50%;
    transform: translate(-50%, -50%);
    .close {
        position: absolute;
        top:0px;
        right: 20px;
        transition: all 200ms;
        font-size: 30px;
        font-weight: bold;
        text-decoration: none;
        color: #333;
    }
    input.mail_filed{
        border: none;
        width: 470px;
        border-bottom: 1px solid #ddd;
        padding: 0;
        height: auto;
        margin: 0 5px;
    }
    .form-group input, .form-group textarea, .form-group select {
        background-color: transparent;
        height: 35px; 
        margin-top: 6px;
    }
    input.mail_filed_one {
        border: 1px solid #ccc;
        width: 100%;
    }
    .form-text-area {
        position: relative;
        border: 1px solid #ccc;
        padding: 5px 15px 0;
        margin-bottom: 15px;
    }
    textarea{
        resize: none;
    }
    .form-text-area .form-group {
        margin-bottom: 0;
    }
    .form-text-area .form-group p {
        margin-bottom: 0;
    }
    .form-text-area .form-group {
        margin-bottom: 5px!important;
    }
    .form-group.head-req p {
        font-size: 15px;
    }
    input#submit_popup {
        margin-top: 15px;
    }
    input:focus {
        text-decoration: none;
        outline: none !important;
        box-shadow: none;
    }
    .form-group {
        margin-bottom: 15px;
    }
    span#success {
        color: green;
        font-size: 18px;
        position: relative;
        float: left;
        width: 100%;
        padding-top: 15px;
    }
}
